select s.id
, so.is_bonus_month
, o.scheduled_at = '10/15/2018'
from barkbox_copy.subscriptions s
left join barkbox_copy.subscription_orders so on so.subscription_id = s.id
left join barkbox_copy.orders o on o.user_id = s.user_id
where s.id in (1486005,1344058,1433546,1589034,1714446,1802373,1667382,1270907,1941518,473098,1910243,1021585,1506305,1727518,1562744,1670649,1379885,1028789,1398699,1772673,190822,2072353,1801047,1059509,1801381,961040,1915282,1469299,2122709,2038326,1423856,1674088,1544907,1514476,1946073,1985317,1347157,1406525,2133310,2048950,1001474,1955410,2082768,1182910,2015302,1667245,1580499,1826446,1030246,2196795,2094943,1539502,2372274,1987899,1868516,1697024,2204393,1833874,2092575,2223641,1817690,2102635,154842,2325780,1626781,1509426,2096566,2268548,2253604,2225007,2085696,2086250,2249611,2340968,1929928,1723763,2328518,1321773,2331599,2327386,2352544,2352573,2353864,236326,2364012,2366967,2368333,2368342,2368544,2368724,2072551,2237500,1787655,2373829,1883802,2353157,1822808,2093402,2224050,1804370,1820057,2229661,2293061,2206500,228188,1608697,1008096,2321968)
group by 1,2,3
